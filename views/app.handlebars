    <style id="style"></style>
    <body class="darkMode">
        <div class="container-fluid fixed-top ">
            <div class="row">
                <div class="col-12">
                    <nav class="navbar bg-primary">
                        <div class="container-fluid">
                            <div class="row justify-content-between w-100">
                                <div>
                                    <a href="/login"><img id="logo" src="/assets/img/face.png" alt="face logo"/></a>
                                </div>
                                <div>
                                    <button id="darkModeButton" type="button" class="btn darkMode">
                                    <icon class="material-icons">brightness_3</icon>
                                    </button>
                                    <a id="upgradeButton" href="/pricing"><div class="btn btn-secondary text-light ml-sm-2 align-middle">Upgrade</div></a>
                                    <a href="/logout"><div class="btn btn-secondary text-light align-middle ml-sm-2 ">Log out</div></a>
                                    <a href="/account"><div class="btn btn-secondary text-light align-middle ml-sm-2 ">Account</div></a>
                                </div>
                            </div>
                        </div>
                    </nav>
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    <nav class="navbar bg-primary" id="makeBookmark">
                    </nav>
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <nav class="navbar bg-primary">
                        <div class="container-fluid">
                            <div class="row">
                                <a class="navbar-brand" href="/login"><img src="/assets/img/face.png" alt="face logo"/></a>
                                <a href="/logout"><div class="btn btn-secondary text-light align-middle">Log out</div></a>
                                <a href="/account"><div class="btn btn-secondary text-light align-middle">Account</div></a>
                            </div>
                        </div>
                    </nav>
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    <nav class="navbar bg-primary">
                    </nav>
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="row">
                <section id="bookmarks" class="col-11">
                </section>
                <div id="ads" class="col-1 m-0 p-0">

                </div>

                <div class="modal fade" id="bookmarkMessage" tabindex="-1" role="dialog" aria-labelledby="updateModalModalLabel" aria-hidden="true">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content darkMode">
                            <div class="modal-header darkMode">
                                <h4><span id="errorMessage" class="modal-title"></span></h4>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <!--<div id="bookmarkMessage"><img id="speech" src="/assets/img/speech.png" alt="speech box"/>
<h4><span id="errorMessage"></span></h4><img id="bookmark" src="assets/img/bookmark.png" alt="bookmark"/>
</div>-->


            </div>
        </div>

        <section id="updateLocation" class="col-11">
        </section>

        <script src="/assets/bundle.js"></script>
